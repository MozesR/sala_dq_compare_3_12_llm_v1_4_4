{
  "badges": {
    "audit": {
      "assign_red": 4,
      "assign_warn": 1,
      "compute_red": 4,
      "compute_warn": 1,
      "impute_red": 4,
      "impute_warn": 1
    }
  },
  "logic": [
    {
      "if": "status IS NULL",
      "then": "assign status = 'unknown'"
    },
    {
      "if": "unit_price IS NULL and currency == 'USD'",
      "then": "impute unit_price = mean(unit_price)"
    },
    {
      "if": "quantity IS NULL",
      "then": "impute quantity = 1"
    },
    {
      "if": "total_amount IS NULL and quantity IS NOT NULL and unit_price IS NOT NULL",
      "then": "compute total_amount = quantity * unit_price"
    },
    {
      "if": "order_id IS NOT NULL and status IS NULL",
      "then": "assign status = 'unknown'"
    },
    {
      "if": "true",
      "then": "assign dq_alert = 'mean_total_amount_shift'"
    }
  ]
}